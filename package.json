{
  "name": "tryp-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "vercel-build": "yarn run missing-translation && yarn run load-translation && next build",
    "build": "next build",
    "analyze": "cross-env ANALYZE=true next build",
    "format": "prettier --ignore-path .gitignore --write .",
    "prepare": "husky install",
    "gen:theme-typings": "chakra-cli tokens ./src/styles/theme.ts",
    "postinstall": "npm run gen:theme-typings",
    "missing-translation": "node ./scripts/build/translations/missing",
    "load-translation": "node ./scripts/build/translations/load",
    "unused-translation": "yarn load-translation && node ./scripts/build/translations/unused",
    "config": "node ./scripts/build/config"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@microsoft/clarity": "^1.0.0",
    "@next/third-parties": "^15.1.1",
    "@stripe/react-stripe-js": "^3.1.1",
    "@stripe/stripe-js": "^5.5.0",
    "@tawk.to/tawk-messenger-react": "^2.0.2",
    "@types/mapbox-gl": "^3.4.1",
    "@vercel/functions": "^2.0.0",
    "accept-language": "^3.0.18",
    "amazon-cognito-identity-js": "^5.2.10",
    "aws-amplify": "^5.0.5",
    "aws-sdk": "^2.1365.0",
    "chakra-react-select": "^4.6.0",
    "crisp-sdk-web": "^1.0.25",
    "date-fns": "^2.28.0",
    "framer-motion": "^11.0.3",
    "html-react-parser": "^3.0.8",
    "intl-pluralrules": "^1.3.1",
    "jotai": "^2.6.1",
    "jotai-scope": "^0.4.1",
    "lodash": "^4.17.21",
    "mapbox-gl": "^3.12.0",
    "next": "^14.2.21",
    "next-intl": "^3.25.3",
    "next-nprogress-bar": "^2.3.12",
    "react": "^18.2.0",
    "react-date-range": "^1.3.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.36.1",
    "react-lottie-player": "^1.5.0",
    "react-map-gl": "^8.0.4",
    "react-phone-input-2": "^2.15.0",
    "react-player": "^2.16.0",
    "react-swipeable": "^7.0.1",
    "react-use-keypress": "^1.3.1",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.4.1",
    "@next/bundle-analyzer": "^12.2.0",
    "@svgr/webpack": "^7.0.0",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-date-range": "^1.4.9",
    "@types/react-dom": "^18.0.5",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.2",
    "eslint": "8.18.0",
    "eslint-config-next": "^13.1.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "next-sitemap": "^4.0.7",
    "node-fetch": "2",
    "prettier": "^2.7.1",
    "typescript": "^5.0.2"
  }
}
